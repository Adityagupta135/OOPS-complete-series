#include<iostream>
using namespace std;


class customer
{
  string name;
  int account_number, balance,age;


  public:

  customer(string a, int b, int c, int age)
  {
name=a;
account_number=b;
balance=c;
this->age = age;

  }


  void display()
  {

cout<<name<<" "<< account_number<<" "<<balance<<" "<<age<<endl;
  }

};



int main()
{
customer A1("Rohit",1,1000,3);
customer A2("Mohit",2,1800,4);
A1.display();
A2.display();
customer A3("Mohan",3,2000,8);
A3.display();

}
