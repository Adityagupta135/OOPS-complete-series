#include<iostream>
#include<fstream>
#include<vector>
#include <algorithm>


using namespace std;


int main()
{

    vector<int>arr(5);
    cout<<"Enter the input:"<<endl;
    for(int i=0; i<5; i++)
    {
           cin>>arr[i];
    }

    //open the file

    ofstream fout;

    fout.open("Zero.txt");  //create the file and open the file
  fout<<"Original data\n";
  for(int i=0; i<5; i++)
  {
    fout<<arr[i]<<" ";
  }
    fout<<"\nSorted data\n";
    fout.close();     //Resources Release

    sort(arr.begin(), arr.end());
     for(int i=0; i<5; i++)
  {
    fout<<arr[i]<<" ";
  }
}
